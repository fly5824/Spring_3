<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace ="com.naver.s3.notice.NoticeDAO">

<select id="getList" resultType="NoticeDTO">
	select * from notice
</select>

<select id = "getSelect" parameterType="NoticeDTO" resultType="NoticeDTO">
	select * from notice where num= #{num}
</select>

<insert id="setInsert" parameterType="NoticeDTO">
	insert into notice values(NOTICE_SEQ.nextval,#{title},#{writer},#{contents},#{hit},sysdate)
</insert>











</mapper>